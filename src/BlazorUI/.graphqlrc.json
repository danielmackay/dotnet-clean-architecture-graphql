{
  "schema": "schema.graphql",
  "documents": "**/*.graphql",
  "extensions": {
    "strawberryShake": {
      "name": "TodoClient",
      "namespace": "BlazorUI.GraphQL",
      "url": "https://localhost:44339/graphql/",
      "records": {
        "inputs": false,
        "entities": true
      },
      "transportProfiles": [
        {
          "default": "Http",
          "subscription": "WebSocket"
        }
      ]
    }
  }
}